<!DOCTYPE html>
<html lang="en">

    <head>
        <%- include("partials/head") %>
        <%- block("stylesheets").toString() %>
    </head>

    <body>
        <!-- PAGE HEADER -->
        <%- include("partials/page-header") %>

        <!-- MAIN NAVBAR -->
        <%- include("partials/main-navbar") %>

        <%- body %>

        <% if (err.length) { %>
        <p id="error-text" style="display: none;"><%= err[0] %></p>
        <script src="/common/scripts/swal-error.js" type="module"></script>
        <% } %>

        <!-- PAGE FOOTER -->
        <%- include("partials/page-footer") %>
        <script src="/common/scripts/vendor/progressbar.min.js" defer></script>
        <script src="https://cdn.jsdelivr.net/npm/axios/dist/axios.min.js" defer></script>
        <script src="//cdn.jsdelivr.net/npm/sweetalert2@11"></script>
        <script src="/common/scripts/navbar-main.js" type="module"></script>
        <%- block("scripts").toString() %>
    </body>

</html>
